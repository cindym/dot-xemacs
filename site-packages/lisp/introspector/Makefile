.SUFFIXES: .el .elc

EMACS ?= xemacs
#EMACS=c:/PROGRA~1/xemacs/XEMACS~1.19/I586-P~1/xemacs.exe

.el.elc:
	$(EMACS) -batch -q -f batch-byte-compile $<

ELCS = 	frame-utils.elc introspector.elc


all:	$(ELCS) auto-autoloads.el custom-load.el

auto-autoloads.el: $(ELCS)
	rm -f auto-autoloads.el
ifeq ($(EMACS),sxemacs)
	$(EMACS) -batch -f batch-create-autoloads .
else
	$(EMACS) -batch -q -no-site-file -l autoload -f batch-update-directory .
endif

custom-load.el: $(ELCS)
	$(EMACS) -batch -vanilla -l cus-dep -f Custom-make-dependencies .
	$(EMACS) -batch -vanilla -f batch-byte-compile custom-load.el

tags:
	etags *.el

clean:
	rm -f *.elc auto-autoloads.el custom-load.el TAGS
